%{
import parse
import swifty_flavors as swift

models = parse.parseModels("sample.yml")

}%
// 
// Model definitions generated by Swako
//

% for model in models:
// ${model.name}
public struct ${model.name} {
% for property in model.properties:
  public var ${property.name}: ${swift.typeNameFromProperty(property)} ${swift.descriptionComment(property.description)}
% end
}

% end